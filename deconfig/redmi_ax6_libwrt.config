##### TARGET #####
CONFIG_TARGET_qualcommax=y
CONFIG_TARGET_qualcommax_ipq807x=y
CONFIG_TARGET_MULTI_PROFILE=y
CONFIG_TARGET_PER_DEVICE_ROOTFS=y
CONFIG_TARGET_DEVICE_qualcommax_ipq807x_DEVICE_redmi_ax6=y
CONFIG_TARGET_ROOTFS_INITRAMFS=n

##### Compile #####
CONFIG_DEVEL=y
CONFIG_CCACHE=y
CONFIG_USE_APK=n

##### BusyBox #####
CONFIG_BUSYBOX_CUSTOM=y
CONFIG_BUSYBOX_CONFIG_TELNET=y

##### IPv6 #####
CONFIG_PACKAGE_ipv6helper=y
CONFIG_PACKAGE_odhcp6c=y
CONFIG_PACKAGE_odhcpd-ipv6only=y
CONFIG_PACKAGE_luci-proto-ipv6=y
CONFIG_PACKAGE_ip6tables-nft=y

##### Firewall / nft #####
CONFIG_PACKAGE_iptables-nft=y

##### NSS Hardware Acceleration #####
CONFIG_FEED_nss_packages=y
CONFIG_PACKAGE_kmod-qca-nss-dp=y
CONFIG_PACKAGE_kmod-qca-nss-ecm=y
CONFIG_PACKAGE_kmod-qca-nss-drv=y
CONFIG_PACKAGE_kmod-qca-mcs=y
CONFIG_PACKAGE_nss-firmware-ipq8074=y

CONFIG_NSS_FIRMWARE_VERSION_11_4=y
CONFIG_NSS_MEM_PROFILE_HIGH=y

# NSS extra (PPPoE OK)
CONFIG_PACKAGE_kmod-qca-nss-drv-pppoe=y

##### Kernel #####
CONFIG_PACKAGE_kmod-tun=y
CONFIG_KERNEL_SKB_RECYCLER=y
CONFIG_KERNEL_IPQ_MEM_PROFILE_256=y

##### UPnP #####
CONFIG_PACKAGE_miniupnpd-nftables=y
CONFIG_PACKAGE_luci-app-upnp=y

##### LuCI Basic #####
CONFIG_PACKAGE_luci=y
CONFIG_PACKAGE_luci-app-autoreboot=y
CONFIG_PACKAGE_luci-app-ttyd=y
CONFIG_PACKAGE_luci-app-wol=y
CONFIG_PACKAGE_luci-app-openclash=y

##### Tools #####
CONFIG_PACKAGE_htop=y
CONFIG_PACKAGE_tcpdump=y
CONFIG_PACKAGE_openssh-sftp-server=y
CONFIG_PACKAGE_xz-utils=y

##### Defaults #####
CONFIG_PACKAGE_default-settings=y
CONFIG_PACKAGE_default-settings-chn=y
